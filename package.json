{
  "name": "bidbay-platform",
  "version": "1.0.0",
  "description": "A web platform for creating and bidding on listings",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "build": "sass src/scss:dist/css",
    "transpile": "babel src -d dist",
    "format": "prettier --write \"src/**/*.js\" ",
    "watch": "sass --watch src/scss:dist/css & live-server",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "prettierCheck": "prettier --check \"src/**/*.js\"",
    "eslintCheck": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ousman37/BidBay-Platform.git"
  },
  "keywords": [
    "web",
    "platform",
    "listings",
    "bidding",
    "marketplace",
    "authentication"
  ],
  "author": "Ethmane Didi <ousman.didibela@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ousman37/BidBay-Platform/issues"
  },
  "homepage": "https://github.com/Ousman37/BidBay-Platform#readme",
  "dependencies": {
    "babel": "^6.23.0",
    "body-parser": "^1.20.1",
    "bootstrap": "^5.2.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "live-server": "^1.1.0",
    "morgan": "^1.10.0",
    "prettierrc": "^0.0.0-5",
    "process-env": "^2.0.2",
    "sass": "^1.58.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.20.11",
    "@testing-library/cypress": "^9.0.0",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5"
  }
}
